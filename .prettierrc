{
	"printWidth": 160,
	"tabWidth": 2,
	"useTabs": false,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"jsxSingleQuote": true,
	"jsxBracketSameLine": false,
	"arrowParens": "always",
	"endOfLine": "lf",
	"quoteProps": "consistent",
	"proseWrap": "preserve",
	"embeddedLanguageFormatting": "auto"
}
